<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval" android:innerRadius="20dp"
            >
            <solid android:color="@color/c53" />

            <stroke android:width="0.1dp" android:color="@color/c53"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval" android:innerRadius="20dp"
            >
            <solid android:color="@color/c49" />

            <stroke android:width="0.1dp" android:color="@color/c49"/>
        </shape>
    </item>

</selector>
